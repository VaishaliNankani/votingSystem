	public static Iterable<SiteJsonSchemaVersionInfo> generateSiteJsonSchemaVersionInfoList() {
		var siteJsonSchemaVersionInfoList = new ArrayList<SiteJsonSchemaVersionInfo>();
		var tableNames = Arrays.asList("NfInstances", "NfStatusMonitor","NrfEventTransactions",
										"NfScreening", "NrfSystemOptions", "NfSubscriptions");
		for(String tableName:tableNames) {
			SiteJsonSchemaVersionInfo siteJsonSchemaVersionInfo = new SiteJsonSchemaVersionInfo();
			siteJsonSchemaVersionInfo.setSiteId("6fbf1bbd-6f4a-4454-a507-b14ef8e1bc6f");
			siteJsonSchemaVersionInfo.setTableName(tableName);
			siteJsonSchemaVersionInfo.setVersion("v2");
			siteJsonSchemaVersionInfoList.add(siteJsonSchemaVersionInfo);
		}
		for(String tableName:tableNames) {
			SiteJsonSchemaVersionInfo siteJsonSchemaVersionInfo = new SiteJsonSchemaVersionInfo();
			siteJsonSchemaVersionInfo.setSiteId("Network");
			siteJsonSchemaVersionInfo.setTableName(tableName);
			siteJsonSchemaVersionInfo.setVersion("v1");
			siteJsonSchemaVersionInfoList.add(siteJsonSchemaVersionInfo);
		}
		return siteJsonSchemaVersionInfoList;
	}
