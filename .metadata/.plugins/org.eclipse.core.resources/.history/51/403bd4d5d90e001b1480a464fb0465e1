	public static SubscriptionData generateSubscriptionDataWithNfInstanceId() {
		SubscriptionData subscriptionData = new SubscriptionData();
		subscriptionData.setNfStatusNotificationUri(URI.create("http://abc.def.com/callback"));
		SubscrCond cond = new SubscrCond();
		cond.setNfInstanceId(UUID.randomUUID().toString());
		subscriptionData.setSubscrCond(cond);
		subscriptionData.setSubscriptionId(UUID.randomUUID().toString().replaceAll("[\\s\\-()]", ""));
		
		List<NotificationEventType> evList = new ArrayList<NotificationEventType>();
		evList.add(NotificationEventType.NF_PROFILE_CHANGED);
		evList.add(NotificationEventType.NF_DEREGISTERED);
		subscriptionData.setReqNotifEvents(evList);
		Calendar c = Calendar.getInstance();
		c.add(Calendar.DATE, 90);
		subscriptionData.setValidityTime(c.getTime());
		return subscriptionData;
	}
	
	public static SubscriptionData generateSubscriptionDataWithNoNfInstanceId() {
		SubscriptionData subscriptionData = new SubscriptionData();
		subscriptionData.setNfStatusNotificationUri(URI.create("http://abc.def.com/callback"));
		List<NotificationEventType> evList = new ArrayList<NotificationEventType>();
		evList.add(NotificationEventType.NF_REGISTERED);
		evList.add(NotificationEventType.NF_DEREGISTERED);
		subscriptionData.setReqNotifEvents(evList);
		
		SubscrCond cond = new SubscrCond();
		cond.setServiceName("some-pcf-service");
		subscriptionData.setSubscrCond(cond);

		

		Calendar c = Calendar.getInstance();
		c.add(Calendar.DATE, 90);
		subscriptionData.setValidityTime(c.getTime());
		return subscriptionData;
	}
	
