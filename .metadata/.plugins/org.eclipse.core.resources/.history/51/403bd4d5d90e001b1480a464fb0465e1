		svc.setRecoveryTime(date);
		return svc;
	}
	
	public static NfHeartBeat generateHeartBeatRecord(NFStatus status) throws IOException {
		NfHeartBeat nfHeartBeat = new NfHeartBeat();
		
		nfHeartBeat.setNfInstanceId(UUID.randomUUID().toString());
		nfHeartBeat.setNfStatus(status);
		nfHeartBeat.setLoad(50);
		nfHeartBeat.setLastHbTimeStamp(new Date());
		nfHeartBeat.setNumHbMissed(0);
		nfHeartBeat.setHeartBeatTimer(30);
		if (status == NFStatus.SUSPENDED) {
			nfHeartBeat.setSuspendedTimestamp(new Date());
		}
		else {			
			nfHeartBeat.setSuspendedTimestamp(0L);
		}
		nfHeartBeat.setLastNotifLoad(50);	
		return nfHeartBeat;		
	}
	
