package com.oracle.oci.votingservice.domain;

import javax.validation.constraints.NotNull;

public class CandidateData {
	public CandidateData(long voterId, String voterName, String address, 
			long phoneNumber, boolean voteCasted) {
		super();
		this.voterId = voterId;
		this.voterName = voterName;
		this.address = address;
		this.phoneNumber = phoneNumber;
		this.voteCasted = voteCasted;
	}
	@NotNull
	private long candidateId;	
	@NotNull
	private String  candidateName = null;
	@NotNull
	private String  party = null;
	@NotNull
	private String constituency  = null;
	

}
