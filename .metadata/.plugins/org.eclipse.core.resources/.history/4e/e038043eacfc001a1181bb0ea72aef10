logging.level.root=${LOG_LEVEL:INFO}
initial.ssl-config.sslenabled=${SSL_ENABLED:true}
initial.ssl-config.port=${SSL_PORT:443}
initial.ssl-config.tlsversion=${TLS_VERSION:TLSv1.2}
initial.ssl-config.cipherSuite=${CIPHER_SUITE:TLS_ECDHE_ECDSA_WITH_AES_128_GCM_SHA256}
initial.ssl-config.privatekey-secret-name=${SSL_PRIVATEKEY_SECRET_NAME:ssl-privatekey-secret}
initial.ssl-config.privatekey-secret-namespace=${SSL_PRIVATEKEY_SECRET_NAMESPACE:ssl}
initial.ssl-config.rsa-privatekey-filename=${SSL_RSA_PRIVATE_KEY_FILENAME:ssl_rsa_private_key.pem}
initial.ssl-config.ecdsa-privatekey-filename=${SSL_ECDSA_PRIVATE_KEY_FILENAME:ssl_ecdsa_private_key.pem}
initial.ssl-config.certificate-secret-name=${SSL_CERTIFICATE_SECRET_NAME:ssl-cert-secret}
initial.ssl-config.certificate-secret-namespace=${SSL_CERTIFICATE_SECRET_NAMESPACE:ssl}
initial.ssl-config.rsa-certificate-filename=${SSL_RSA_CERTIFICATE_FILENAME:ssl_rsa_certificate.crt}
initial.ssl-config.ecdsa-certificate-filename=${SSL_ECDSA_CERTIFICATE_FILENAME:ssl_ecdsa_certificate.crt}
initial.ssl-config.initial-algorithm=${SSL_INITIAL_ALGORITHM:ES256}
initial.ssl-config.ca-bundle-secret-name=${SSL_CA_BUNDLE_SECRET_NAME:ssl-cabundle-secret}
initial.ssl-config.ca-bundle-secret-namespace=${SSL_CA_BUNDLE_SECRET_NAMESPACE:ssl}
initial.ssl-config.ca-bundle-filename=${SSL_CA_BUNDLE_FILENAME:ssl_cabundle.crt}
initial.ssl-config.keystore-password-filename=${SSL_KEYSTORE_PASSWORD_FILENAME:ssl_keystore.txt}
initial.ssl-config.keystore-password-secret-name=${SSL_KEYSTORE_PASSWORD_SECRET_NAME:ssl-keystore-secret}
initial.ssl-config.keystore-password-secret-namespace=${SSL_KEYSTORE_PASSWORD_SECRET_NAMESPACE:ssl}
initial.ssl-config.truststore-password-filename=${SSL_TRUSTSTORE_PASSWORD_FILENAME:ssl_truststore.txt}
initial.ssl-config.truststore-password-secret-name=${SSL_TRUSTSTORE_PASSWORD_SECRET_NAME:ssl-truststore-secret}
initial.ssl-config.truststore-password-secret-namespace=${SSL_TRUSTSTORE_PASSWORD_SECRET_NAMESPACE:ssl}
initial.oauth-config.keystore-password-filename=${OAUTH_KEYSTORE_PASSWORD_FILENAME:oauth_keystore_password.txt}
initial.oauth-config.keystore-password-secret-name=${OAUTH_KEYSTORE_PASSWORD_SECRET_NAME:oauth-keystore-secret}
initial.oauth-config.keystore-password-secret-namespace=${OAUTH_KEYSTORE_PASSWORD_SECRET_NAMESPACE:oauth}
initial.oauth-config.certificate-secret-name=${OAUTH_CERTIFICATE_SECRET_NAME:oauth-cert-secret}
initial.oauth-config.certificate-secret-namespace=${OAUTH_CERTIFICATE_SECRET_NAMESPACE:oauth}
initial.oauth-config.rsa-privatekey-filename=${OAUTH_RSA_PRIVATE_KEY_FILENAME:oauth_rsa_private_key.pem}
initial.oauth-config.ecdsa-privatekey-filename=${OAUTH_ECDSA_PRIVATE_KEY_FILENAME:oauth_ecdsa_private_key.pem}
initial.oauth-config.privatekey-secret-name=${OAUTH_PRIVATEKEY_SECRET_NAME:oauth-privatekey-secret}
initial.oauth-config.privatekey-secret-namespace=${OAUTH_PRIVATEKEY_SECRET_NAMESPACE:oauth}
initial.oauth-config.initial-algorithm=${OAUTH_INITIAL_ALGORITHM:ES256}
server.port=${SERVER_PORT:9443}
server.http.port=8080
server.redirect.http=false
server.http2.enabled=true
initial.global-config.initssl=${INIT_SSL:false}
